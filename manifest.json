{
    "name": "Kudos for Google Chat extension app",
    "description": "Extension to share feedback on real time on your google chat channel",
    "version" :"1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage"
      ],
    "icons": { 
        "16": "assets/icon16.png",
        "48": "assets/icon64.png",
        "128": "assets/icon128.png" 
    },
    "options_page": "options.html"
}